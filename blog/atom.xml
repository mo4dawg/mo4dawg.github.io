---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Kevin Connell's Blog</title>
	<link href="http://kevinjconnell.com"/>
	<link type="application/atom+xml" rel="self" href="http://kevinjconnell.com/atom.xml"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://kevinjconnell.com/blog</id>
	<author>
		<name>Kevin Connell</name>
		<email>kevinjconnell@hotmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<id>http://kevinjconnell.com{{ post.id }}</id>
			<link type="text/html" rel="alternate" href="http://kevinjconnell.com{{ post.url }}"/
			<title>{{ post.title }}</title>
			<link href="http://kevinjconnell.com{{ post.url }}"/>
			<published>{{ post.date | date_to_xmlschema }}</published>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<author>
				<name>Kevin Connell</name>
				<email>kevinjconnell@hotmail.com</email>
			</author>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}
</feed>
